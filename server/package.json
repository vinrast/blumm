{
  "name": "blumm",
  "version": "1.0.0",
  "description": "Blog personal",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "tsc -P ./tsconfig.json && tsc-alias",
    "predev": "npm run migrate && npm run seeder",
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' --ignore 'node_modules'",
    "migrate": "sequelize db:migrate",
    "seeder": "sequelize db:seed:all",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "start": "node .",
    "prod": "npm run build && npm run start",
    "pretest": "sequelize db:migrate",
    "test": "jest --detectOpenHandles",
    "posttest": "sequelize db:migrate:undo:all",
    "test:watch": "jest --watch --coverage=false --detectOpenHandles",
    "test:coverage": "jest --watchAll --coverage --no-cache --detectOpenHandles"
  },
  "keywords": [],
  "author": "vincen santaella",
  "license": "ISC",
  "devDependencies": {
    "@keenondrums/singleton": "1.0.0",
    "@types/adm-zip": "^0.5.0",
    "@types/axios": "0.14.0",
    "@types/bull": "^4.10.0",
    "@types/express": "4.17.9",
    "@types/factory-girl": "5.0.12",
    "@types/faker": "5.1.5",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.14.1",
    "@types/nodemailer": "^6.4.0",
    "@types/sequelize": "4.28.9",
    "@types/superagent": "4.1.10",
    "@types/supertest": "^2.0.11",
    "@types/validator": "13.1.0",
    "@types/validatorjs": "3.15.0",
    "@types/xml-js": "^1.0.0",
    "@types/xml2js": "^0.4.11",
    "factory-girl": "5.0.4",
    "faker": "5.1.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "6.6.0",
    "supertest": "4.0.2",
    "ts-jest": "29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.360.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.7",
    "@types/pdfmake": "^0.2.2",
    "adm-zip": "^0.5.10",
    "axios": "0.21.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "cors": "2.8.5",
    "dotenv": "^16.3.1",
    "express": "4.17.1",
    "jest": "^29.4.3",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.3",
    "nodemailer": "^6.6.1",
    "pdfmake": "^0.2.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "redis": "^4.6.7",
    "sequelize": "6.3.5",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-jest": "0.0.1",
    "uuidv4": "6.2.6",
    "validatorjs": "3.22.1",
    "yup": "^1.2.0"
  }
